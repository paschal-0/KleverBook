{
  "expo": {
    "name": "KleverBook",
    "slug": "KleverBook",
    "version": "1.0.0",
    "orientation": "portrait",
    "assetBundlePatterns": [
      "src/assets/**",
      "assets/**"
    ],
    "icon": "./src/assets/images/logo/img.png",
    "scheme": "kleverbook",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.kleverbook.ios"
    },
    "android": {
      "package": "com.yourcompany.kleverbook",
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./src/assets/images/logo/img.png",
        "backgroundImage": "./src/assets/images/logo/img.png",
        "monochromeImage": "./src/assets/images/logo/img.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "INTERNET",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "ACCESS_NETWORK_STATE",
        "WAKE_LOCK"
      ]
    },
    "web": {
      "output": "static",
      "favicon": "./src/assets/images/logo/img.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/logo/img.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "gradleProperties": {
              "newArchEnabled": "true"
            }
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "3d9acaa2-74ca-491f-91d1-dbf3a8cb4111"
      }
    }
  }
}
